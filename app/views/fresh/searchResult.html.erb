<!DOCTYPE html>
<html ng-app = 'app'>
  <head>
    <title>BootstrapCode</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="application.css">
  </head>

  
  <body ng-controller="FreshlyController">

    <nav class="navbar navbar-inverse">
      <div class="container-fluid">

        <div class="navbar-header">        
            <div class="inner-addon left-addon menu">
              <i class="glyphicon glyphicon-search"></i>
              <form ng-submit="search()">
                  <input type="text" ng-model="search_term" class="form-control" placeholder = "Search" />
              </form>
            </div>
        </div>


        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#myNavbar">
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>                
        </button>        

        <div class="collapse navbar-collapse" id="myNavbar">
          <ul class="nav navbar-nav" >
            <li class="active"><a ng-click="show_home()" href="#" style="width:160px; text-align:center;">Home</a></li>
            <li class="dropdown" > 
              <a class="dropdown-toggle" data-toggle="dropdown" href="#" style="width:160px; text-align:center;">Categories 
                <span class="caret"></span>
              </a>
              <ul class="dropdown-menu">
                <li ng-repeat="category in categories">
                  <a href="#" ng-click="show_category(category); $event.preventDefault(); $event.stopPropagation();">{{category}}</a>
                </li> 
              </ul>
            </li>           
          </ul>

          <ul class="nav navbar-nav navbar-right">
            <li><a href="#"><span class="glyphicon glyphicon-user"></span> Sign Up</a></li>
            <li><a href="#"><span class="glyphicon glyphicon-log-in"></span> Login</a></li>
          </ul>

        </div>
      </div>
    </nav>
  
