    <ul class="pager">
      <li ng-click="show_all()">
        <a href="#" style="margin-right:85px;"> Show All </a>
      </li>
    </ul>



    
  <script src="js/jquery.min.js"></script>
  <script src="js/bootstrap.min.js"></script>
  <script src="js/angular.js"></script>
  <script src="app.js"></script>

</body>
</html>
